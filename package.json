{
  "name": "piterpy-web",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bootstrap": "^3.3.7",
    "bootswatch": "^3.3.7",
    "react": "^15.4.2",
    "react-bootstrap": "^0.30.7",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.8",
    "react-scroll-up-button": "^1.5.7",
    "react-youtube": "^7.3.0",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "reselect": "^2.5.4"
  },
  "devDependencies": {
    "node-sass": "^4.5.0",
    "npm-run-all": "^4.0.2",
    "react-scripts": "0.9.3",
    "redux-devtools": "^3.3.2"
  },
  "scripts": {
    "build-css": "node-sass src/ -o src/",
    "watch-css": "npm run build-css && node-sass src/ -o src/ --watch",
    "react-scripts-bin": "NODE_PATH='src/' react-scripts",
    "start-js": "npm run react-scripts-bin start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && npm run react-scripts-bin build",
    "test": "npm run react-scripts-bin test --env=jsdom",
    "eject": "npm run react-scripts-bin eject"
  },
  "proxy": "http://localhost:8000/"
}
